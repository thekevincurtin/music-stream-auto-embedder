{
  "name": "Music Stream Auto Embedder",
  "description": "Embeds player widget for elements with music URLs",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "hook.js"
  },
  "permissions": [
      "scripting",
      "tabs"
  ],
  "host_permissions": [
    "http://*/*", 
    "https://*/*"
  ]
}